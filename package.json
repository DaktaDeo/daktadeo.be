{
  "name": "daktadeo-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "build:do": "rm -r _static 2>/dev/null; nuxt build && nuxt export && mv dist _static && (rm _static/README.md || true)",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "start": "nuxt start",
    "start:do": "node sammy.js; serve -s _static -l tcp://0.0.0.0:${PORT:-8080} -n",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{css,vue}": "stylelint",
    "*.{js,vue}": "eslint"
  },
  "dependencies": {
    "@aceforth/nuxt-optimized-images": "^1.3.0",
    "@nuxt/content": "^1.9.0",
    "@nuxt/image": "^0.0.4",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/browserconfig": "^0.0.13",
    "@nuxtjs/color-mode": "^2.0.1",
    "@nuxtjs/device": "^1.2.7",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/google-fonts": "^1.2.0",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/svg": "^0.1.12",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^9",
    "clipboard": "^2.0.6",
    "core-js": "^3.6.5",
    "lodash": "^4.17.20",
    "marked": "^1.2.5",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "nuxt": "^2.14.6",
    "nuxt-i18n": "^6.16.0",
    "postcss": "^7",
    "prism-themes": "^1.5.0",
    "tailwind-css-variables": "^2.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "theme-colors": "^0.0.5",
    "vue-agile": "^1.1.3",
    "vue-scrollactive": "^0.9.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^3.1.0",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fibers": "^5.0.0",
    "husky": "^4.3.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "imagemin-svgo": "^8.0.0",
    "jest": "^26.5.0",
    "lint-staged": "^10.4.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.1.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.5",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "vue-jest": "^3.0.4",
    "webpack": "^4.0.0"
  }
}
